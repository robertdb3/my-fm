{
  "name": "music-cable-box",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "turbo run dev --parallel --filter=@music-cable-box/shared --filter=@music-cable-box/api --filter=@music-cable-box/web",
